		<image48x48>deploy/androidImages/icons/icon48.png</image48x48>
		<image72x72>deploy/androidImages/icons/icon72.png</image72x72>
	</icon>
	<supportedProfiles>mobileDevice</supportedProfiles>
	<android> 
	    <manifestAdditions> 
	        <![CDATA[ 
	            <manifest>
					<uses-permission android:name="android.permission.INTERNET" />
